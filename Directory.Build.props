<Project>
  <Import Project="./build/sign.props" />
  <Import Project="./build/version.props" />
  <PropertyGroup>
    <LangVersion>latest</LangVersion>
    <Nullable>enable</Nullable>
    <PackageId>HTTPie</PackageId>
    <Title>HTTPie</Title>
    <Description>dotnet httpie</Description>
    <PackageTags>HTTPie http https curl rest</PackageTags>
    <PackageIconUrl>https://avatars3.githubusercontent.com/u/7604648</PackageIconUrl>    
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryType>git</RepositoryType>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <RepositoryUrl>https://github.com/WeihanLi/dotnet-httpie</RepositoryUrl>
    <Authors>WeihanLi</Authors>
    <Copyright>Copyright 2021 (c) WeihanLi</Copyright>
  </PropertyGroup>
</Project>
